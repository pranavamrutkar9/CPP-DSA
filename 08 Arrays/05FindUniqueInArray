#include <iostream>
using namespace std;

int findUnique(int *arr, int size)
{
    for (int i = 0; i < size; i++)
    {
        int flag = 0;
        for (int j = i+1; j < size; j++)
        {
            if (arr[i]==arr[j])
            {
                flag = 1;
            }
            
        }
        if (flag == 0)
        {
            return arr[i];
        }
    }
}

int main(){
    int a[]={2,3,1,6,3,6,2};
    int n = (sizeof(a)/sizeof(a[0]));

    for (int i = 0; i < n; i++)
    {
        cout<<a[i];
    }
    cout<<endl;
    cout<<findUnique(a, n);

    
}
